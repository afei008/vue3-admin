    "vue": "^3.2.47",
    "vue-router": "^4.1.6",
    "vue3-draggable-resizable": "^1.6.5",
    "vuedraggable": "^2.24.3",
    "vxe-table": "^4.3.10",
    "xe-utils": "^3.5.7"
