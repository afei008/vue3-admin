    "@rushstack/eslint-patch": "^1.2.0",
    "@types/jsdom": "^21.1.0",
    "@types/node": "^18.13.0",
    "@types/overlayscrollbars": "^1.12.1",
    "@types/path-browserify": "^1.0.0",
    "@vitejs/plugin-vue": "^4.0.0",
