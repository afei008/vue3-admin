    "@antv/x6-plugin-transform": "^2.1.5",
    "@antv/x6-vue-shape": "^2.0.9",
    "@micro-zoe/micro-app": "^0.8.10",
    "@tinymce/tinymce-vue": "^5.0.0",
    "ant-design-vue": "^3.2.15",
