    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-plugin-cypress": "^2.12.1",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-vue": "^9.9.0",
    "jsdom": "^21.1.0",
    "less": "^4.1.3",
    "npm-run-all": "^4.1.5",
    "postcss-html": "^1.5.0",
    "postcss-scss": "^4.0.6",
